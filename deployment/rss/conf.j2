{
    "name": "{{ rssVersion }}",
    "config": {
        "main": "net.atos.entng.rss.Rss",
        "port": 8028,
        "app-name": "Rss",
        {% if csrfToken %}"csrf-token": true,{% endif %}
        "app-address": "/rss",
        "app-icon": "rss-large",
        "app-type" : "WIDGET",
        "host": "https://{{ host }}",
        "ssl": true,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode": "HTTP",
        "app-registry.port": 8012,
        "mode": "prod",
        "entcore.port": 8009
    }
}